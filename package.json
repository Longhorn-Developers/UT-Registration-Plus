        "@unocss/vite": "^0.63.6",
        "@vitejs/plugin-react": "^5.1.4",
        "clsx": "^2.1.1",
        "conventional-changelog": "^7.1.1",
        "date-fns": "^4.1.0",
        "highcharts": "^12.5.0",
